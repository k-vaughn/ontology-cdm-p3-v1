    <owl:Ontology rdf:about="https://w3id.org/citydata/part3/v1/">
    	<!-- Identity -->
    	<dcterms:title>Information technology - City data model - Part 3: Service level concepts - Transport</dcterms:title>
    	<skos:definition>This ontology specifies the service-level concepts defined for the transport service of the city data model.</skos:definition>
        <rdfs:seeAlso rdf:resource="https://www.iso.org/standard/87441.html"/>
        <dcterms:bibliographicCitation>ISO/IEC 5087-3</dcterms:bibliographicCitation>
    	<vann:preferredNamespaceUri>https://w3id.org/citydata/part3/v1/</vann:preferredNamespaceUri>
    	<vann:preferredNamespacePrefix>cdm3</vann:preferredNamespacePrefix>
    		
    	<!-- Provenance -->
    	<dcterms:creator>Kenneth Vaughn</dcterms:creator>
