        <!-- which includes the following cascade of imports:
            TransportNetworkPattern
                Core(3)
                TransportInfrastructurePattern
                    InfrastructurePattern
                        ContactPattern
                            CityPattern
                                LandUsePattern
                                    CodePattern
                                        Core(2)
                                        OrganizationStructurePattern
                                            AgentPattern
                                                GenericPropertiesPattern
                                                ResourcePattern
                                                    ActivityPattern
                                                        ChangePattern
                                                        SpatialLocPattern
                                                            MereologyPattern
                                                    CityUnitsPattern
                                                        ISO21972
        -->
        <owl:imports rdf:resource="https://w3id.org/citydata/part3/v1/PedestrianNetworkPattern/"/>
        <!-- which includes the following cascade of imports:
            TransportNetworkPattern (as above)
        -->
        <owl:imports rdf:resource="https://w3id.org/citydata/part3/v1/PublicTransportSystemPattern/"/>
        <!-- which includes the following cascade of imports:
            TransportNetworkPattern (as above)
        -->
        <owl:imports rdf:resource="https://w3id.org/citydata/part3/v1/RailNetworkPattern/"/>
        <!-- which includes the following cascade of imports:
            TransportNetworkPattern (as above)
        -->
        <owl:imports rdf:resource="https://w3id.org/citydata/part3/v1/RoadNetworkPattern/"/>
        <!-- which includes the following cascade of imports:
            TransportNetworkPattern (as above)
        -->
        <owl:imports rdf:resource="https://w3id.org/citydata/part3/v1/TransportAlertPattern/"/>
        <!-- which includes the following cascade of imports:
            TransportNetworkPattern (as above)
            AlertPattern
                Core(3)
                CodePattern (as above)
        -->
        <owl:imports rdf:resource="https://w3id.org/citydata/part3/v1/TravelCorridorPattern/"/>
        <!-- which includes the following cascade of imports:
            TransportNetworkPattern (as above)
        -->
