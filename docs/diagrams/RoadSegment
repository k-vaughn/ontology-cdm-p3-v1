// Diagram for RoadSegment
digraph {
	graph [overlap=false rankdir=TB splines=true]
	node [fontname=Arial fontsize=12 margin=0 shape=none]
	edge [fontname=Arial fontsize=10]
	TravelledWaySegment [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER"><B>TravelledWaySegment</B></TD></TR></TABLE>> margin=0]
	transinfras_RoadSegment [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER"><B>transinfras:RoadSegment</B></TD></TR></TABLE>> margin=0]
	{
		rank=max
		RoadSegment [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER" PORT="e"><B>RoadSegment</B></TD></TR><TR><TD ALIGN="LEFT">numLanes: nonNegativeInteger «exactly 1, only»</TD></TR><TR><TD ALIGN="LEFT">width: nonNegativeInteger «exactly 1, only»</TD></TR></TABLE>> margin=0]
	}
	subgraph cluster_associated {
		label="" style=invis
		Invis [label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD></TD></TR></TABLE>> margin=0 style=invis]
		RoadLane [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER"><B>RoadLane</B></TD></TR></TABLE>> margin=0]
		RoadLink [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER"><B>RoadLink</B></TD></TR></TABLE>> margin=0]
		RoadSection [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER"><B>RoadSection</B></TD></TR></TABLE>> margin=0]
		code_Code [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER"><B>code:Code</B></TD></TR></TABLE>> margin=0]
	}
	RoadSegment -> TravelledWaySegment [arrowhead=onormal style=solid]
	RoadSegment -> transinfras_RoadSegment [arrowhead=onormal style=solid]
	RoadSegment -> Invis [style=invis]
	Invis -> RoadLane [style=invis]
	RoadLane -> RoadLink [style=invis]
	RoadLink -> RoadSection [style=invis]
	RoadSection -> code_Code [style=invis]
	RoadSegment -> code_Code [label="«max 1, only» pavementType" arrowhead=normal style=solid]
	RoadSegment -> RoadLane [label="«min 1, only» onProperty: partwhole:hasProperPart" arrowhead=normal style=dashed]
	Union_partwhole_properPartOf [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1" BGCOLOR="lightyellow"><TR><TD ALIGN="CENTER">«unionOf»<BR/>[RoadLink or RoadSection]</TD></TR></TABLE>> margin=0]
	RoadLink [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER"><B>RoadLink</B></TD></TR></TABLE>> margin=0]
	Union_partwhole_properPartOf -> RoadLink [label=member arrowhead=normal style=dotted]
	RoadSection [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER"><B>RoadSection</B></TD></TR></TABLE>> margin=0]
	Union_partwhole_properPartOf -> RoadSection [label=member arrowhead=normal style=dotted]
	RoadSegment -> Union_partwhole_properPartOf [label="«only» onProperty: partwhole:properPartOf" arrowhead=normal style=dashed]
	RoadSegment -> RoadLink [label="«exactly 1» onProperty: partwhole:properPartOf" arrowhead=normal style=dashed]
}
